<h2>Our Contacts - <%= link_to 'New', new_contacts_path %></h2>
<table>
  <thead>
    <th>ID</th>
    <th>Name</th>
    <th>Email</th>
    <th>Number</th>
    <th>Actions</th>
  </thead>
  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.id %></td>
        <td><%= contact.name %></td>
        <td><%= contact.email %></td>
        <td><%= contact.phone_number %></td>
        <td>
          <%= link_to 'Edit', edit_contact_path(contact.id), method: :get %>
          <%= link_to 'Delete', delete_contact_path(contact.id), method: :delete %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
